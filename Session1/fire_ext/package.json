{
    "name": "my-extension",
    "displayName": "My Extension",
    "description": "Adds a fire emoji to long lines",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": ["Other"],
    "activationEvents": ["onCommand:my-extension.addFire"],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "my-extension.addFire",
            "title": "Add Fire to Long Lines"
        }]
    }
}